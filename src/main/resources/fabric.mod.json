{
  "schemaVersion": 1,
  "id": "commandlog",
  "version": "1.0.0",
  "name": "Command Logger",
  "description": "Logs commands sent by you",
  "authors": [
    "kyrptonaught"
  ],
  "contact": {
    "homepage": "https://github.com/kyrptonaught/commandlog",
    "sources": "https://github.com/kyrptonaught/commandlog"
  },
  "license": "MIT",
  "icon": "assets/commandlog/icon.png",
